{
    "name": "rayanlevert/s3",
    "description": "Simpler with typed methods wrapper over the S3 SDK from AWS for handling buckets",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "php",
        "s3",
        "wrapper"
    ],
    "support": {
        "issues": "https://github.com/rayanlevert/s3/issues"
    },
    "authors": [
        {
            "name": "Rayan Levert",
            "email": "rayanlevert@msn.com"
        },
        {
            "name": "Yann Nave",
            "email": "yann.nave@group-dis.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "aws/aws-sdk-php": "^3.293"
    },
    "require-dev": {
        "phpmd/phpmd": "@stable",
        "squizlabs/php_codesniffer": "@stable",
        "phpstan/phpstan": "@stable",
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "RayanLevert\\S3\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RayanLevert\\S3\\Tests\\": "tests"
        }
    }
}
